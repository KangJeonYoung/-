<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kjy.mapper.MemberMapper">

	<resultMap type="com.kjy.domain.MemberVO" id="memberMap">
		<id property="userid" column="userid" />
		<result property="userpw" column="userpw"/>
		<result property="userName" column="userName"/>
		<result property="regDate" column="regDate"/>
		<result property="updateDate" column="updateDate"/>
		<collection property="authList" resultMap="authMap">
		</collection>
	</resultMap>
	
	<resultMap type="com.kjy.domain.AuthVO" id="authMap">
		<result property="userid" column="userid" />
		<result property="auth" column="auth" />
	</resultMap>
	
	<select id="read" resultMap="memberMap">
		SELECT mem.userid, userpw, username, enabled, regdate, updatedate, auth FROM tbl_member mem LEFT OUTER JOIN tbl_member_auth auth on mem.userid = auth.userid WHERE mem.userid = #{userid}
	</select>
	
	
</mapper>